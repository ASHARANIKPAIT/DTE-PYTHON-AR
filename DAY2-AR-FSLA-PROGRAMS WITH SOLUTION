#python3 program to demonstrate a function with and without argumennts
def function1():                                                                                   #function defination of function1 with no arguments
  print(f" THIS IS A FUNCTION WITH NO ARGUMENTS")                                                  #printing this is a function with no arguments
def function2(message):                                                                            #function defination of function2 with one argument message
  print(f"THIS IS A FUNCTION WITH {message}")#printing this is a function with one argument
def main():#function defination of main function
  print(f"INSIDE MAIN FUNCTION")#printing inside main function
  function1()#calling function1 without passing any arguments
  function2("ONE ARGUMENT")#calling function2 by passing one argument
if(__name__=="__main__"):
  main()#calling main function